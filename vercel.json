{
    "version": 2,
    "builds": [
      {
        "src": "tabletap/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "tabletap/wsgi.py"
      }
    ],
    "env": {
      "PYTHONUNBUFFERED": "1"
    }
  }